#include<stdio.h>
int main()
{
           int i=0,r,b[8]={0},num;
           printf("enter value of num:");
           scanf("%d",&num);
	   	while (num != 0)
		{ r=num % 2;
		  b[i]=r;
		  num=num / 2;
		  i ++;
		} 
	   
            i--;
            for(i=7;i>=0;i--)
	     {
               printf("%d",b[i]);
	     }
}   
           
